   "source": [
    "def get_pairs(word):\n",
    "\n",
    "    pais = set()\n",
    "    prev_char = word[0]\n",
    "    for char in word[1:]:\n",
    "        pairs.add((prev_char,char))\n",
    "        prev_char = char\n",
    "    return pairs\n"
   ]
  }
